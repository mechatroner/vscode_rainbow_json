{
	"scopeName": "source.json.rainbow",
	"injectionSelector": "L:source.json -comment",
	"patterns": [
		{
			"match": "\"[^\"\\\\]*(?:\\\\.[^\"\\\\]*)*\"\\s*:",
			"name": "meta.structure.dictionary.key.json.rainbow"
		},
		{
			"match": "\"[^\"\\\\]*(?:\\\\.[^\"\\\\]*)*\"",
			"name": "meta.structure.dictionary.value.json.rainbow"
		},
		{
			"match": "-?(?:0|[1-9]\\d*)(?:\\.\\d+)?(?:[eE][+-]?\\d+)?",
			"name": "meta.structure.dictionary.value.json.rainbow"
		},
		{
			"match": "\\b(?:true|false|null)\\b",
			"name": "meta.structure.dictionary.value.json.rainbow"
		},
		{
			"match": "[\\[\\]{}:,]",
			"name": "meta.structure.json.rainbow"
		}
	]
}
