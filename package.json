{
    "name": "rainbow-json",
    "displayName": "Rainbow JSON",
    "description": "Highlight JSON keys with rainbow colors for better readability.",
    "version": "0.0.1",
    "publisher": "vscode-samples",
    "repository": "https://github.com/Microsoft/vscode-extension-samples/helloworld-minimal-sample",
    "engines": {
        "vscode": "^1.100.0"
    },
    "activationEvents": [
        "onLanguage:json",
        "onLanguage:jsonl"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "rainbow-json.Enable",
                "title": "Rainbow JSON: Enable Highlighting"
            },
            {
                "command": "rainbow-json.Disable",
                "title": "Rainbow JSON: Disable Highlighting"
            }
        ],
        "configurationDefaults": {
          "[json][jsonl]": {
            "editor.semanticHighlighting.enabled": true
          }
        },
        "semanticTokenScopes": [
          {
            "language": "json",
            "scopes": {
              "rainbow1": ["source.json"],
              "rainbow2": ["keyword.rainbow2"],
              "rainbow3": ["entity.name.function.rainbow3"],
              "rainbow4": ["comment.rainbow4"],
              "rainbow5": ["string.rainbow5"],
              "rainbow6": ["variable.parameter.rainbow6"],
              "rainbow7": ["constant.numeric.rainbow7"],
              "rainbow8": ["entity.name.type.rainbow8"],
              "rainbow9": ["markup.bold.rainbow9"],
              "rainbow10": ["invalid.rainbow10"],
              "comment": ["comment"]
            }
          },
          {
            "language": "jsonl",
            "scopes": {
              "rainbow1": ["source.json.lines"],
              "rainbow2": ["keyword.rainbow2"],
              "rainbow3": ["entity.name.function.rainbow3"],
              "rainbow4": ["comment.rainbow4"],
              "rainbow5": ["string.rainbow5"],
              "rainbow6": ["variable.parameter.rainbow6"],
              "rainbow7": ["constant.numeric.rainbow7"],
              "rainbow8": ["entity.name.type.rainbow8"],
              "rainbow9": ["markup.bold.rainbow9"],
              "rainbow10": ["invalid.rainbow10"],
              "comment": ["comment"]
            }
          }
        ]
    },
    "scripts": {},
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@stylistic/eslint-plugin": "^2.9.0",
        "@types/vscode": "^1.100.0",
        "eslint": "^9.13.0",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.0"
    }
}